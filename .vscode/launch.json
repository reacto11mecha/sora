{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Development mode web interface",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn dev",
      "cwd": "${workspaceFolder}/apps/web/",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "E2E web interface w/ ui",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn e2e:ui",
      "cwd": "${workspaceFolder}/apps/web/",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
